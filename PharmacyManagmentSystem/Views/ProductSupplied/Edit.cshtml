@model PharmacyManagmentSystem.Models.productsupplied

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>productsupplied</h4>
        <hr />
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.productSuppliedId)

        <div class="form-group">
            @Html.LabelFor(model => model.productDetailId, "productDetailId", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("productDetailId", String.Empty)
                @Html.ValidationMessageFor(model => model.productDetailId)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.supplierId, "supplierId", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("supplierId", String.Empty)
                @Html.ValidationMessageFor(model => model.supplierId)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-info btn-sm" />
                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary btn-sm" })
            </div>
        </div>
    </div>
}
